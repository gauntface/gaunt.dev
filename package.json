{
  "name": "gaunt.dev",
  "type": "module",
  "scripts": {
    "build": "npm run build-hugo && npm run process-html",
    "build-hugo": "npm run clean && hugo && rm -rf tmp && cp -r public tmp",
    "build-quick": "npm run clean && cp -r tmp public && npm run process-html",
    "clean": "rm -rf public",
    "process-html": "npm run html-assets && npm run minify-html",
    "html-assets": "htmlassets --vimeo=$VIMEO_TOKEN",
    "minify-html": "minify -r --match=\\.html -o ./public ./public/*",
    "dev": "hugo server --disableFastRender",
    "test": "npm run build && npm run ava",
    "test-integration": "npx ava ./test/integration/*.js",
    "ava": "npx ava ./test/*.js"
  },
  "private": true,
  "devDependencies": {
    "@sentry/browser": "7.37.1",
    "@sentry/tracing": "7.37.1",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "ava": "5.2.0",
    "mkdirp": "2.1.3",
    "performance-leaderboard": "10.0.0",
    "puppeteer": "19.6.3",
    "static-server": "2.2.1",
    "typescript": "4.9.5"
  }
}
